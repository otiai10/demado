{
  "manifest_version": 3,
  "name": "demado",
  "description": "ブラゲーのランチャー",
  "version": "2.0.14",
  "permissions": [
    "activeTab",
    "downloads",
    "scripting",
    "storage",
    "tabs"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": [
      "<all_urls>"
    ]
  },
  "icons": {
    "128": "icons/128.png"
  },
  "action": {
    "default_icon": {
      "128": "icons/128.png"
    },
    "default_title": "demado",
    "default_popup": "index.html#popup"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "index.html#options"
}
